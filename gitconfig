[alias]
    lg = log --graph --pretty=format:'%Cred%h%Creset -%C(auto)%d%Creset %s %Cgreen(%cr by %an)%Creset' --abbrev-commit --date=relative
    lga = log --graph --pretty=format:'%Cred%h%Creset -%C(auto)%d%Creset %s %Cgreen(%cr by %an)%Creset' --abbrev-commit --date=relative --all
    di = diff
    ss = stash save
    sp = stash pop
    st = status
[merge]
	conflictstyle = diff3
[push]
	default = simple
